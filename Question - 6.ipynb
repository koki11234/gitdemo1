   "source": [
    "**6)Find the product of odd indexed numbers from the list.**"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "d4341fa3",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "product of odd indexed numbers: 48\n"
     ]
    }
   ],
   "source": [
    "a=[1,2,3,4,5,6]\n",
    "odd=1\n",
    "for i in range(len(a)):\n",
    "    if(i%2!=0):\n",
    "        odd=odd*a[i]\n",
    "print(\"product of odd indexed numbers:\",odd)\n"
   ]
  }
